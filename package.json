{
  "name": "ytlinks",
  "version": "1.0.0",
  "description": "YouTube search scraper app",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "cd client && npm start",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "install-client": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run install-client && npm run build"
  },
  "keywords": [
    "youtube",
    "scraper",
    "express",
    "react"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "destroy": "^1.2.0",
    "express": "^4.18.2",
    "play-dl": "^1.9.7",
    "puppeteer": "^24.9.0",
    "ytdl-core": "^4.11.5"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1"
  }
}
